                    //    $('#Viewport').attr('content', 'initial-scale=' + ratio + ', maximum-scale=' + ratio + ', minimum-scale=' + ratio + ', user-scalable=yes, width=' + ww);
                    //} else { //regular size
                    //    $('#Viewport').attr('content', 'initial-scale=1.0, maximum-scale=2, minimum-scale=1.0, user-scalable=yes, width=' + ww);
                    //}

                    //$('#main').css('width', '950px');
                    //$('nav').css('width', '860px');
                    //$('#site_content').css('width', '840px');
                    //$('.content').css({ 'width': '450px', 'margin-left': '15px' });
                    //$('footer').css('width', '860px');
                    //$('#logo').css('width', '860px');
                    //$('.slogantext').css('width', '860px');
                    //$('.imgLogo').css('margin', '0 50px 0 50px');
                    //$('table tr td').css('font-size', '25px');
                    //$('#BtnSubmit').css({ 'width': '200px', 'height': '70px' });
                    //$('#sidebar_container').css('width', '370px');
                    //$('.sidebar').css({ 'width': '330px', 'font-size': '27px' });
                    //$('#txtvalidate').css('width', '335px');

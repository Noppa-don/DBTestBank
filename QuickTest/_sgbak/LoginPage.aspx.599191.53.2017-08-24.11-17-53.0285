                else {
                    //page transition
                    FadePageTransition();
                    $(':submit').click(function () {
                        FadePageTransitionOut();
                    });
                    $('#btnPractice').click(function () {
                        FadePageTransitionOut();
                    });
                }


                $('ul.sf-menu').sooperfish();

                // Login With QR Code
                $('#ImgBtnQRLogin').click(function (e) {
                    e.preventDefault();
                    $('#QRImg').css({ 'background': 'url("")', '    background-repeat': 'no-repeat', 'background-size': 'cover' });
                    $('#filePic').val("");
                    $('#blockQRLogin').show();
                });
                $('#blockQRLogin').click(function (e) {
                    //console.log(e.target);
                    if ($(e.target).is('.QRLoginOverlay')) {
                        $(this).hide();
                    }
                });
                $('#closeQRLogin').click(function () {
                    $('#blockQRLogin').hide();
